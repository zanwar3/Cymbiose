{
  "name": "cymbiose-backend",
  "version": "1.0.0",
  "description": "Cymbiose AI Backend - Diagnostic Support API",
  "main": "dist/server.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/server.js",
    "dev": "ts-node-dev --respawn --transpile-only src/server.ts",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "db:generate": "prisma generate",
    "db:push": "prisma db push",
    "db:migrate": "prisma migrate dev",
    "db:studio": "prisma studio",
    "db:seed": "ts-node prisma/seed.ts",
    "db:reset": "prisma migrate reset --force",
    "db:health": "ts-node src/utils/dbHealth.ts",
    "setup": "./scripts/setup-db.sh",
    "lint": "tsc --noEmit",
    "clean": "rm -rf dist node_modules/.cache"
  },
  "dependencies": {
    "express": "^4.18.2",
    "@prisma/client": "^5.7.0",
    "cors": "^2.8.5",
    "helmet": "^7.1.0",
    "express-rate-limit": "^7.1.5",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.0",
    "zod": "^3.22.4",
    "dotenv": "^16.3.1"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/node": "^20.10.0",
    "@types/jest": "^29.5.8",
    "@types/cors": "^2.8.17",
    "typescript": "^5.3.2",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.1",
    "@types/swagger-jsdoc": "^6.0.4",
    "@types/swagger-ui-express": "^4.1.6",
    "prisma": "^5.7.0",
    "supertest": "^6.3.3",
    "@types/supertest": "^2.0.16"
  },
  "prisma": {
    "seed": "ts-node prisma/seed.ts"
  },
  "keywords": ["mental-health", "ai", "diagnostic", "api"],
  "author": "Cymbiose AI",
  "license": "MIT"
}
